
1 3 4 5 7 


#include <iostream>
using namespace std;

int main() {
    int arr[]={1 ,4 ,5 ,3 ,7};
    int n = 5;
    for(int i=0;i<n;i++) {
        for(int j=0;j<n-i-1;j++) {
            if(arr[j+1]<arr[j]) {
                swap(arr[j],arr[j+1]);
            }
        }
    }
    for(int i=0;i<n;i++) {
        cout<<arr[i]<<" ";
    }

    return 0;
}
